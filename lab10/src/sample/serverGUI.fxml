<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:controller="sample.serverController"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <TextArea fx:id="textArea" GridPane.rowIndex="0" GridPane.columnIndex="1"
    prefWidth="400" prefHeight="400"></TextArea>
    <Button text="Exit" GridPane.rowIndex="2" GridPane.columnIndex="0" onAction="#exit"></Button>
</GridPane>
